common:
	php:
		date.timezone: Europe/Prague


	services:
		cacheStorage:
			class: Nette\Caching\Storages\MemoryStorage


	extensions:
		console: Kdyby\Console\DI\ConsoleExtension


	translation:
		whitelist: [cs, sk, en]
		resolvers:
			session: on


v22 < common:
	nette:
		security:
			frames: null

		session:
			autoStart: false

v23 < common:
	http:
		frames: null

	session:
		autoStart: false
